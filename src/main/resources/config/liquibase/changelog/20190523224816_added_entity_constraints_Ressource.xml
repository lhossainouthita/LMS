<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Ressource.
    -->
    <changeSet id="20190523224816-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="exercice_id"
                                 baseTableName="ressource"
                                 constraintName="fk_ressource_exercice_id"
                                 referencedColumnNames="id"
                                 referencedTableName="exercice"/>

        <addForeignKeyConstraint baseColumnNames="cours_id"
                                 baseTableName="ressource"
                                 constraintName="fk_ressource_cours_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cours"/>

    </changeSet>
</databaseChangeLog>
